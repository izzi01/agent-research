# AgentOS Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://agno:your-password@postgres-service:5432/marketing_automation

# AI Model API Keys
ANTHROPIC_API_KEY=sk-ant-api03-your-key-here

# Video Generation API Keys
SIMPLIFIED_API_KEY=your-simplified-key
HEYGEN_API_KEY=your-heygen-key
DID_API_KEY=your-did-key
RUNWAY_API_KEY=your-runway-key

# Vietnamese TTS API
VBEE_API_KEY=your-vbee-key

# Platform API Credentials
FACEBOOK_ACCESS_TOKEN=your-facebook-token
FACEBOOK_PAGE_ID=your-page-id
TIKTOK_ACCESS_TOKEN=your-tiktok-token
TIKTOK_CLIENT_KEY=your-tiktok-client-key
SHOPEE_PARTNER_ID=your-shopee-partner-id
SHOPEE_PARTNER_KEY=your-shopee-partner-key
SHOPEE_SHOP_ID=your-shop-id

# Trend Monitoring
TICKERTRENDS_API_KEY=your-tickertrends-key

# Application Configuration
AGENTOS_ENV=development
LOG_LEVEL=info
ENABLE_METRICS=true

# OpenAI (for embeddings - optional)
OPENAI_API_KEY=your-openai-key-for-embeddings
